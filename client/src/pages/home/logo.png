/*  -------- HASH LINK TARGET POSITIONING ---------------------
    -  Corrects spacing for inner page jump links.
    -  Target link id name needs to include the string "goto".
----------------------------------------------------------------------------------  */
[id*="goto"]:before { 
  display: block; 
  content: " "; 
  margin-top: -160px; 
  height: 160px; 
  visibility: hidden; 
  pointer-events: none;
}
